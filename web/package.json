		"@solid-primitives/websocket": "^1.2.2",
		"@solidjs/meta": "^0.29.4",
		"@solidjs/router": "^0.15.2",
		"@tanstack/solid-query": "^5.62.12",
		"@tanstack/solid-query-devtools": "^5.62.12",
		"@tanstack/solid-virtual": "3.11.2",
