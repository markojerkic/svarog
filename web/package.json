{
    "name": "example-with-tailwindcss",
    "type": "module",
    "scripts": {
        "dev": "vinxi dev",
        "build": "vinxi build --mode production",
        "start": "vinxi start",
        "lint": "biome lint --error-on-warnings ./{src,test}",
        "lint:fix": "biome lint --write --unsafe ./{src,test}",
        "format:check": "biome format ./{src,test}",
        "format": "biome format --write ./{src,test}",
        "test": "vitest",
        "test:watch": "vitest --watch"
    },
    "dependencies": {
        "@solid-primitives/scheduled": "^1.4.3",
        "@solid-primitives/websocket": "^1.2.2",
        "@solidjs/meta": "^0.29.4",
        "@solidjs/router": "^0.13.6",
        "@solidjs/start": "^1.0.6",
        "@tanstack/solid-query": "^5.51.21",
        "@tanstack/solid-query-devtools": "^5.51.21",
        "@tanstack/solid-virtual": "3.8.1",
        "autoprefixer": "^10.4.20",
        "postcss": "^8.4.40",
        "solid-js": "^1.8.19",
        "tailwindcss": "^3.4.7",
        "vinxi": "^0.3.14"
    },
    "engines": {
        "node": ">=18"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.8.3",
        "tailwind-scrollbar": "^3.1.0",
        "vitest": "^1.6.0"
    }
}
